### Aliases


alias sha1='openssl sha1'
alias z='zsh --login'
alias htop='sudo htop'
alias sg='shotgun -p 3000'
alias sgp='shotgun -s puma -p 3000 config.ru'
alias eject_usb='diskutil unmount'
alias tmux-named='tmux new -s'
alias brew-uses='brew uses --installed'
alias coffee6='coffee --nodejs --harmony'
alias be='bundle exec'


### Aliases For Mac Applications

alias psql='/Applications/Postgres.app/Contents/Versions/9.4/bin/psql -p5432
psql'
alias julia='exec "/Applications/Julia-0.4.0.app/Contents/Resources/julia/bin/julia"'
