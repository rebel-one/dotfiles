# storing pasword credentials in osx keychain w/:
# git config --global credential.helper osxkeychain


[user]
	name = {{username}}
	email = {{email}}
[alias]
	branch-out = checkout -b
	switch-branch = checkout
	com = commit -am
	co = checkout
	sb = checkout
	br = branch
	st = status
[apply]
	whitespace = nowarn
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore_global
	editor = mate -w
[credential]
	helper = osxkeychain
